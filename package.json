{
  "name": "minimalcoinscom",
  "private": true,
  "version": "0.1.0",
  "description": "A simple cryptocurrency ticker site.",
  "repository": "https://github.com/MaxMilton/MinimalCoins.com",
  "bugs": "https://github.com/MaxMilton/MinimalCoins.com/issues",
  "homepage": "https://minimalcoins.com",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "keywords": [
    "bitcoin",
    "cryptocurrency",
    "maxmilton",
    "minna-ui",
    "postcss",
    "svelte",
    "web-performance"
  ],
  "scripts": {
    "dev": "APP_RELEASE=$(git describe --always --dirty='-dev') NODE_ENV=development rollup -c --watch",
    "build": "APP_RELEASE=$(git describe --always --dirty='-dev') NODE_ENV=production rollup -c",
    "lint:css": "stylelint **/*.{css,html} --cache --cache-location=/tmp/",
    "lint:js": "eslint --ignore-path .gitignore --ext js,html .",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test-ci": "yarn test --coverage --runInBand --ci"
  },
  "dependencies": {
    "@minna-ui/css": "0.2.0",
    "svelte": "2.7.2"
  },
  "devDependencies": {
    "@minna-ui/eslint-config": "0.2.0",
    "@minna-ui/jest-config": "0.2.0",
    "@minna-ui/postcss-config": "0.2.0",
    "@minna-ui/stylelint-config": "0.1.0",
    "@minna-ui/svelte-preprocess-markup": "0.2.0",
    "@minna-ui/svelte-preprocess-style": "0.2.0",
    "clean-css": "4.1.11",
    "codecov": "3.0.2",
    "eslint": "4.19.1",
    "jest": "23.1.0",
    "postcss": "6.0.22",
    "rollup": "0.60.7",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-svelte": "4.1.0",
    "rollup-plugin-terser": "1.0.1",
    "stylelint": "9.3.0"
  }
}
